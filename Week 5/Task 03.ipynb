    "import numpy as np\n",
    "\n",
    "even = np.arange(1,11)\n",
    "\n",
    "for num in even:\n",
    "    if num%2 == 1:\n",
    "        even = np.delete(even, np.where(even == num))\n",
    "\n",
    "print(even)"
